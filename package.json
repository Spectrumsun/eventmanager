{
  "name": "server",
  "version": "1.15.2",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production sequelize db:migrate && yarn run build && yarn run webpack:build && node build/server",
    "test": "NODE_ENV=test yarn run migrations && NODE_ENV=test yarn run start:test",
    "start:test": "nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 50000 --exit --require babel-core/register server/test/**/*.test.js",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "server:coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev:all": "NODE_ENV=development concurrently --kill-others \"yarn run webpack:watch\" \"yarn run dev:server\"",
    "dev:server": "nodemon --ignore lib --exec  babel-node server/server",
    "build": "rm -rf build && mkdir build && babel ./server -d ./build ./server -s",
    "webpack:watch": "webpack --watch --config webpack.dev.config.js --progress --profile --color",
    "webpack:build": "webpack --config webpack.prod.config.js",
    "test:client": "jest -u && codecov",
    "test:e2e": "nightwatch",
    "coverages": "yarn run server:coverage && yarn run test:client",
    "all:test": "yarn test && yarn run test:client"
  },
  "author": "Taiwo Sunday",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cloudinary": "^1.17.0",
    "compression-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "node-sass": "^7.0.0",
    "nodemailer": "^6.3.1",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.12.1",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.0",
    "sequelize": "^5.21.2",
    "swagger-ui-express": "^4.1.2",
    "toastr": "^2.1.4",
    "url-loader": "^2.2.0",
    "uuid-random": "^1.3.0"
  },
